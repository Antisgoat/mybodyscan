{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18 <21"
  },
  "scripts": {
    "dev": "vite",
    "build:web": "MODE=production node scripts/generate-app-config.mjs && vite build",
    "build:native": "MODE=native node scripts/generate-app-config.mjs && vite build --mode native",
    "build:native:release": "MODE=native MBS_NATIVE_RELEASE=1 node scripts/generate-app-config.mjs && vite build --mode native",
    "build": "MODE=production node scripts/generate-app-config.mjs && vite build",
    "build:strict": "MODE=production node scripts/generate-app-config.mjs && vite build",
    "build:prod": "MODE=production node scripts/generate-app-config.mjs && node scripts/validate-programs.mjs && vite build && npm run check:bundle && npm run check:storage-rest && node scripts/print-build-tag.js",
    "build:dev": "MODE=development node scripts/generate-app-config.mjs && vite build --mode development",
    "build:resilient": "npm run build || (npm cache clean --force && rm -rf node_modules && npm install --prefer-online && npm run build)",
    "doctor": "bash scripts/doctor.sh",
    "ios:doctor": "bash scripts/ios-doctor.sh",
    "ios:open": "open ios/App/App.xcworkspace",
    "ios:ensure-public": "node scripts/ensure-ios-public.mjs",
    "preios:sync": "node scripts/assert-repo-root.mjs && node scripts/ensure-ios-public.mjs",
    "ios:sync": "node scripts/assert-repo-root.mjs && node scripts/assert-no-native-firebase-auth.mjs && npm run build:native && npx cap sync ios && node scripts/assert-no-native-firebase-auth.mjs && node scripts/assert-ios-public-bundle.mjs && (cd ios/App && pod install)",
    "ios:reset": "bash ios/scripts/ios_reset.sh",
    "ios:build:debug": "bash ios/scripts/ios_build_debug.sh",
    "ios:build:release": "bash ios/scripts/ios_build_release.sh",
    "ios:archive": "bash ios/scripts/ios_archive.sh",
    "ios:run": "npm run ios:sync && npm run ios:open",
    "ios:clean": "node scripts/ios-clean.mjs",
    "ios:assets": "npx @capacitor/assets generate --ios",
    "verify:native": "node scripts/verify-native-bundle.mjs",
    "verify:native-bundle": "node scripts/verify-native-bundle.mjs",
    "postinstall": "npm run ios:ensure-public",
    "lint": "eslint .",
    "check:bundle": "node scripts/check-bundle.js",
    "check:storage-rest": "node scripts/check-storage-rest.js",
    "preview": "vite preview",
    "sync:ios": "npm run ios:sync",
    "sync:android": "npm run build:native && npx cap sync android",
    "build:ios": "bash scripts/build-ios.sh",
    "build:android": "bash scripts/build-android.sh",
    "clean:all": "bash scripts/clean-all.sh",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:rules": "npm --prefix tests/rules install --no-audit --no-fund --prefer-online && npm run test --prefix tests/rules",
    "emulators": "firebase emulators:start --only firestore,auth,storage,functions --import ./emulator-data --export-on-exit",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "pretest:e2e": "playwright install --with-deps",
    "lighthouse": "lhci autorun",
    "check:headers": "bash scripts/check-headers.sh $PREVIEW_URL",
    "seed:pricing": "node scripts/seedPricing.cjs",
    "format": "prettier -w .",
    "typecheck": "tsc -p . --noEmit",
    "build:verify": "npm run typecheck && vite build",
    "qa": "bash scripts/qa.sh && node scripts/dev-qa.mjs",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "ci:clean": "npm config set fetch-retry-maxtimeout 600000 && npm cache clean --force && rimraf node_modules && npm ci --prefer-online",
    "ci-or-install": "npm ci || npm install --prefer-online --no-audit --no-fund",
    "verify:local": "npm run build && npm --prefix functions run build",
    "verify:scan": "node scripts/run-verify-scan.mjs",
    "rules:sync": "node scripts/sync-firestore-rules.mjs",
    "rules:check": "node scripts/sync-firestore-rules.mjs --check",
    "dev:check": "vite --open /system/check",
    "smoke": "bash scripts/smoke.sh",
    "smoke:native": "node scripts/smoke-native.mjs",
    "smoke:auth": "bash ops/smoke-auth.sh",
    "smoke:fn": "bash ops/smoke-fn.sh",
    "smoke:fns": "bash ops/smoke-fn.sh",
    "smoke:idtk": "bash ops/idtk-smoke.sh",
    "probe": "node scripts/probe.mjs",
    "audit:graph": "node scripts/audit-graph.mjs > reports/import-graph.json",
    "audit:report": "node scripts/generate-audit-report.mjs",
    "audit:all": "npm run audit:report",
    "storage:cors:set": "bash scripts/storage-cors-set.sh",
    "storage:cors:get": "bash scripts/storage-cors-get.sh"
  },
