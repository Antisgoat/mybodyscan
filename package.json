{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "dev:emulators": "npm run emulators:start",
    "dev:e2e": "npm run build && (npm run preview -- --host=0.0.0.0 --port=4173 & PREVIEW_PID=$!; wait-on http://127.0.0.1:4173; BASE_URL=http://127.0.0.1:4173 npm run test:e2e; TEST_EXIT=$?; kill $PREVIEW_PID; exit $TEST_EXIT)",
    "seed:dev": "tsx scripts/test-seed.ts",
    "build": "vite build",
    "postbuild": "node scripts/write-health-json.mjs",
    "build:prod": "node scripts/validate-programs.mjs && vite build && node scripts/write-health-json.mjs && node scripts/print-build-tag.js",
    "build:dev": "vite build --mode development",
    "build:resilient": "npm run build || (npm cache clean --force && rm -rf node_modules && npm install --prefer-online && npm run build)",
    "build:all": "npm run build && npm --prefix functions run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:rules": "npm run test --prefix tests/rules",
    "emulators:start": "firebase emulators:start --only firestore,auth,functions --import ./emulator-data --export-on-exit",
    "emulators:test": "firebase emulators:exec --only firestore,auth,functions \"vitest run --config tests/integration/vitest.config.ts\"",
    "emulators": "npm run emulators:start",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "playwright test --reporter=list",
    "e2e:report": "playwright show-report",
    "pretest:e2e": "playwright install --with-deps",
    "e2e": "playwright test --headed || (echo 'Headless mode failed, trying with xvfb...' && xvfb-run -a playwright test || echo 'E2E tests skipped - no display available')",
    "lighthouse": "lhci autorun",
    "check:headers": "bash scripts/check-headers.sh $PREVIEW_URL",
    "seed:pricing": "node scripts/seedPricing.cjs",
    "format": "prettier -w .",
    "typecheck": "tsc -b --noEmit",
    "qa": "bash scripts/qa.sh && node scripts/dev-qa.mjs",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "ci:clean": "npm config set fetch-retry-maxtimeout 600000 && npm cache clean --force && rimraf node_modules && npm ci --prefer-online",
    "ci-or-install": "npm ci || npm install --prefer-online --no-audit --no-fund",
    "verify:local": "npm run build && npm --prefix functions run build",
    "rules:verify": "npm run test --prefix tests/rules",
    "dev:check": "vite --open /system/check",
    "smoke": "node scripts/smoke-check.mjs"
  },
  "dependencies": {
    "@capacitor/cli": "^6.2.1",
