  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18 <21"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "node scripts/validate-programs.mjs && vite build && npm run check:bundle && node scripts/print-build-tag.js",
    "build:dev": "vite build --mode development",
    "build:resilient": "npm run build || (npm cache clean --force && rm -rf node_modules && npm install --prefer-online && npm run build)",
    "lint": "eslint .",
    "check:bundle": "node scripts/check-bundle.js",
    "preview": "vite preview",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:rules": "npm run test --prefix tests/rules",
    "emulators": "firebase emulators:start --only firestore,auth,storage,functions --import ./emulator-data --export-on-exit",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "pretest:e2e": "playwright install --with-deps",
    "lighthouse": "lhci autorun",
    "check:headers": "bash scripts/check-headers.sh $PREVIEW_URL",
    "seed:pricing": "node scripts/seedPricing.cjs",
    "format": "prettier -w .",
    "typecheck": "tsc -b --noEmit",
    "qa": "bash scripts/qa.sh && node scripts/dev-qa.mjs",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "ci:clean": "npm config set fetch-retry-maxtimeout 600000 && npm cache clean --force && rimraf node_modules && npm ci --prefer-online",
    "ci-or-install": "npm ci || npm install --prefer-online --no-audit --no-fund",
    "verify:local": "npm run build && npm --prefix functions run build",
    "dev:check": "vite --open /system/check",
    "smoke": "bash scripts/smoke.sh",
    "smoke:auth": "bash ops/smoke-auth.sh",
    "smoke:fn": "bash ops/smoke-fn.sh",
    "smoke:fns": "bash ops/smoke-fn.sh",
    "smoke:idtk": "bash ops/idtk-smoke.sh",
    "probe": "node scripts/probe.mjs"
  },
