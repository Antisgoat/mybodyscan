{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node", 
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "declaration": false,
    "sourceMap": true,
    "noEmit": false
  },
  "include": ["src/**/*.ts"]
}