{
  "type": "module",
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc --project ./tsconfig.json",
    "build:resilient": "npm run build || (npm cache clean --force && rm -rf node_modules && npm install --prefer-online && npm run build)",
    "ci-or-install": "npm ci || npm install --prefer-online --no-audit --no-fund",
    "deploy:functions": "npm run build && firebase deploy --only functions --project mybodyscan-f3daf",
    "set:staff": "ts-node scripts/setStaffClaim.ts"
  },
  "dependencies": {
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^5.1.0",
    "stripe": "^16.0.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "@types/node": "^20.11.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2"
  }
}
